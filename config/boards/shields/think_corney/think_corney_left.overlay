#include "think_corney.dtsi"

&kscan0 {
    col-gpios = <&pro_micro 5 GPIO_ACTIVE_LOW>,
            <&pro_micro 6 GPIO_ACTIVE_LOW>,
            <&pro_micro 7 GPIO_ACTIVE_LOW>,
            <&pro_micro 8 GPIO_ACTIVE_LOW>,
            <&pro_micro 9 GPIO_ACTIVE_LOW>,
            <&pro_micro 10 GPIO_ACTIVE_LOW>;
};

/* Left half; shift its columns by 6 in the composite matrix */
&default_transform {
    col-offset = <6>;
};
